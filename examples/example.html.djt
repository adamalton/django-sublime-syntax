
{% extends 'mrt/pages/semantic.html' %}
{% load static %}

{% block head %}
{{ block.super }}
<title>{{ my_page_title|title }}</title>
<link rel="stylesheet" type="text/css" href="{% static 'lib/cool/ok.css' %}" />
{% endblock %}

{% block body %}
